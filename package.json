{
  "name": "filesystem-utilities",
  "version": "2.1.3",
  "description": "File system utilities for Node.js",
  "main": "index.js",
  "gypfile": true,
  "scripts": {
    "start-linux": "tsc & node dev/linux.js",
    "debug-linux": "tsc & node --nolazy --inspect-brk=9229 dev/linux.js",
    "start-windows": "tsc & node dev/windows.js",
    "debug-windows": "tsc & node --nolazy --inspect-brk=9229 dev/windows.js",
    "build:dev": "tsc & node-gyp build --debug",
    "build": "tsc & node-gyp build",
    "rebuild:dev": "tsc & node-gyp rebuild --debug",
    "rebuild": "tsc & node-gyp rebuild",
    "clean": "node-gyp clean"
  },
  "types": "filesystem-utilities.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uriegel/filesystem-utils.git"
  },
  "keywords": [
    "node",
    "addon",
    "cpp"
  ],
  "author": "Uwe Riegel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uriegel/filesystem-utils/issues"
  },
  "homepage": "https://github.com/uriegel/filesystem-utils#readme",
  "dependencies": {
    "node-addon-api": "^3.0.2"
  },
  "devDependencies": {
    "bindings": "^1.5.0",
    "typescript": "^4.4.4"
  }
}
